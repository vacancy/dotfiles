DIR=$(readlink -f `pwd`)
echo "http://monday.csail.mit.edu/cgi-bin/xiuming/file_explorer.cgi/?dir=${DIR}&patterns_show=*&patterns_highlight=&w=400&n=3&showmedia=1"
